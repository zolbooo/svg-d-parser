{
  "type": "module",
  "name": "svg-d-parser",
  "version": "1.0.0",
  "description": "A parser for d attribute of SVG paths",
  "main": "./lib/cjs/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "build": "npx tsc && npx rollup --config rollup.config.cjs.js && npx rollup --config rollup.config.esm.js && ./scripts/fixup.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zolbooo/svg-d-parser.git"
  },
  "keywords": [
    "svg",
    "path",
    "d",
    "bezier-curve",
    "graphics",
    "vector-graphics"
  ],
  "author": "Zolboobayar Gantumur <zolboobayar@metainc.mn>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zolbooo/svg-d-parser/issues"
  },
  "homepage": "https://github.com/zolbooo/svg-d-parser#readme",
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "rollup": "^3.23.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
